{
    "generators": {
        "@nx/angular:application": {
            "style": "scss",
            "linter": "eslint",
            "unitTestRunner": "jest",
            "e2eTestRunner": "cypress"
        },
        "@nx/angular:library": {
            "linter": "eslint",
            "unitTestRunner": "jest"
        },
        "@nx/angular:component": {
            "style": "scss"
        },
        "@nx/react": {
            "application": {
                "style": "scss",
                "linter": "eslint",
                "bundler": "vite",
                "babel": true
            },
            "component": {
                "style": "scss"
            },
            "library": {
                "style": "scss",
                "linter": "eslint"
            }
        }
    },
    "defaultProject": "example-app-angular",
    "$schema": "./node_modules/nx/schemas/nx-schema.json",
    "targetDefaults": {
        "build": {
            "dependsOn": ["^build"],
            "inputs": ["production", "^production"],
            "cache": true
        },
        "e2e": {
            "inputs": ["default", "^production"],
            "cache": true
        },
        "stylelint": {
            "cache": true
        },
        "@nx/vite:test": {
            "inputs": ["default", "^production", "{workspaceRoot}/jest.preset.ts"],
            "cache": true
        },
        "@nx/jest:jest": {
            "inputs": ["default", "^production", "{workspaceRoot}/jest.preset.ts"],
            "cache": true,
            "options": {
                "passWithNoTests": true
            },
            "configurations": {
                "ci": {
                    "ci": true,
                    "codeCoverage": true
                }
            }
        },
        "@nx/eslint:lint": {
            "inputs": ["default", "{workspaceRoot}/.eslintrc.json"],
            "cache": true
        }
    },
    "namedInputs": {
        "default": ["{projectRoot}/**/*", "sharedGlobals"],
        "sharedGlobals": ["{workspaceRoot}/.stylelintrc.json", "{workspaceRoot}/tsconfig.json", "{workspaceRoot}/tsconfig.json"],
        "production": [
            "default",
            "!{projectRoot}/.eslintrc.json",
            "!{projectRoot}/**/?(*.)+(spec|test).[jt]s?(x)?(.snap)",
            "!{projectRoot}/tsconfig.spec.json",
            "!{projectRoot}/jest.config.[jt]s",
            "!{projectRoot}/src/test-setup.[jt]s"
        ]
    },
    "nxCloudAccessToken": "NDdjOTllNzYtM2QzNi00MjY0LWE0N2MtMjZkYWNkN2M5YjdjfHJlYWQtd3JpdGU=",
    "useInferencePlugins": false,
    "defaultBase": "develop"
}
